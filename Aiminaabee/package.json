  "private": true,
  "scripts": {
    "android": "yarn android:adb && npx react-native run-android",
    "android:release": "yarn android --variant release",
    "android:adb": "adb reverse tcp:8081 tcp:8081",
    "xcode": "open ios/aiminaabee.xcworkspace",
    "ios": "npx react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "ios:pods": "npx pod-install",
    "test": "jest",
    "lint": "eslint .",
    "types:generate": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes=**/*.tsx --endpoint http://localhost:8080/graphql --tagName=gql --outputFlat src/generated --passthroughCustomScalars --customScalarsPrefix Bee",
    "schema:download": "apollo schema:download --endpoint http://localhost:8080/graphql"
  },
